bool canSortArray(vector<int>& nums) {
        int n=nums.size();
        for(int i=0;i<n;i++){
            for(int j=0;j<n-1;j++){
                int k=nums[j];
                int cnt=0;
                while(k>0){
                    k=(k&(k-1));
                    cnt++;
                }

                k=nums[j+1];
                int cnt1=0;
                while(k>0){
                    k=(k&(k-1));
                    cnt1++;
                }

                //condition case..
                if(cnt==cnt1 && nums[j]>nums[j+1]) swap(nums[j],nums[j+1]);

            }
        }

        for(int i=1;i<n;i++){

            if(nums[i]<nums[i-1]) return false;
        }
        return true;
    }
